package com.practice;

public class ClassA 
{	
	void explicit()
	{
		int i1=1000;
		Integer i2=i1;
		Integer i3=Integer.valueOf(i1);
		System.out.println(i3);
		Integer i4=new Integer(10);
		int i5=i4;
		System.out.println(i5);
	}
	
	public static void main(String[] args) 
	{
		new ClassA().explicit();

	}
}
