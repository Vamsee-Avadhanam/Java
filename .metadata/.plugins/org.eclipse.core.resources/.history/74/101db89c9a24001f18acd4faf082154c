package com.phenom;

import java.util.Scanner;

public class ClassA {

	public static void main(String[] args)
	{

		Scanner sc=new Scanner(System.in);
		
		int n=sc.nextInt();
		
		int firstterm=0;
		int secondterm=1;
		int nextterm = firstterm+secondterm;;
		for(int i=0;i<=n;i++)
			 nextterm = firstterm+secondterm;
        System.out.println(nextterm);
	}

}

