package com.practice;

public class ClassA 
{	
	public ClassA()
	{
		int a=10;
		System.out.println("Hello world");
		System.out.println(a);
	}
	
	ClassA(int x)
	{
		System.out.println(x);
	
	
	
	
	
	public static void main(String[] args) 
	{
		new ClassA();
		new ClassA('A');
	}
}
