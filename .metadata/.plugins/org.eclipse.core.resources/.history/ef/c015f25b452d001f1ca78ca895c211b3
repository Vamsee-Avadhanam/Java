package com.pack1;

//Basic program on an Array
public class ClassA {

	
	    public static void main(String[] args) {
	        int[] numbers = {4, 5, 6, 7, 8, 4, 6, 9};

	        System.out.println("Duplicate elements:");
	        for (int i = 0; i < numbers.length; i++) {
	            for (int j = i + 1; j < numbers.length; j++) {
	                if (numbers[i] == numbers[j]) {
	                    System.out.println(numbers[i]);
	                }
	            }
	        }
	    }
	

}


/*
 * O/P: 
 *  First element: 5 
 *  Third element: 15
 *  Last element: 30 
 *  Modified second element: 12
 *  All elements in the array: 5 12 15 20 25 30
 *  Sum of all elements:
 *  Average of all elements: 17.833333333333332
 *  Maximum element: 30 
 *  Minimum element: 5
 */
