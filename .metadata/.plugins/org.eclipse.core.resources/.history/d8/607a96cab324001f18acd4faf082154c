package com.phenom;

import java.util.Scanner;

public class ClassA {

	public static void main(String[] args)
	{

		Scanner sc=new Scanner(System.in);
		
		int n=sc.nextInt();
		
		int firstterm=0;
		int secondterm=1;
		
		for(int i=0;i<=n;i++)
			  n=firstterm+secondterm;
		
		firstterm=secondterm;
		secondterm=n;
		
	}

}

